<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <!-- QWeb Reports -->
    <report
        id="pricetag"
        model="report.pricetag"
        string="Pricetags"
        report_type="qweb-pdf"
        name="sp_product_pricetag.report_pricetag"
        file="sp_product_pricetag.report_pricetag"
        attachment_use="False"
    />

    <record id="sp_product_pricetag.pricetag" model="ir.actions.report.xml">
        <field name="paperformat_id" ref="sp_product_pricetag.paperformat_pricetag"/>
        <field name="attachment_use" eval="0"/>
    </record>

    <report
        id="pricetag_no_price"
        model="report.pricetag_no_price"
        string="Pricetags without price"
        report_type="qweb-pdf"
        name="sp_product_pricetag.report_pricetag_no_price"
        file="sp_product_pricetag.report_pricetag_no_price"
        attachment_use="False"
    />

    <record id="sp_product_pricetag.pricetag_no_price" model="ir.actions.report.xml">
        <field name="paperformat_id" ref="sp_product_pricetag.paperformat_pricetag"/>
        <field name="attachment_use" eval="0"/>
    </record>

    <report
        id="pricetag_vegetables"
        model="report.pricetag_vegetables"
        string="Vegetable Pricetags"
        report_type="qweb-pdf"
        name="sp_product_pricetag.report_pricetag_vegetables"
        file="sp_product_pricetag.report_pricetag_vegetables"
        attachment_use="False"
    />

    <record id="sp_product_pricetag.pricetag_vegetables" model="ir.actions.report.xml">
        <field name="paperformat_id" ref="sp_product_pricetag.paperformat_pricetag_vegetables"/>
        <field name="attachment_use" eval="0"/>
    </record>
</odoo>